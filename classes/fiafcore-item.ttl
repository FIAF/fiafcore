    dc:source "FIAF Cataloguing Manual 3.1.5.2"^^xsd:string ;
    rdfs:domain fiaf:Item ;
    rdfs:range fiaf:Status .

<https://fiafcore.org/ontology/hasStock> a owl:ObjectProperty ;
