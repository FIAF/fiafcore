@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix fiaf: <https://ontology.fiafcore.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Class

fiaf:BroadcastStandard a owl:Class ;
    rdfs:label "Broadcast Standard"@en ;
    rdfs:label "Norma de transmisión de video"@es ;
    rdfs:label "Standard de télédiffusion"@fr ;
    dc:source "FIAF Moving Image Cataloguing Manual 3.1.5.10"@en ;
    dc:source "Manual FIAF de Catalogación de Imágenes en Movimiento 3.1.5.10"@es ;
    dc:source "Manuel de catalogage des images animées de la FIAF 3.1.5.10"@fr .

# Subclasses

fiaf:NTSC a owl:Class ;
    rdfs:label "NTSC"@en ;
    rdfs:label "NTSC"@es ;
    rdfs:label "NTSC"@fr ;
    dc:source "FIAF Moving Image Cataloguing Manual 3.1.5.10"@en ;
    dc:source "Manual FIAF de Catalogación de Imágenes en Movimiento 3.1.5.10"@es ;
    dc:source "Manuel de catalogage des images animées de la FIAF 3.1.5.10"@fr ;
    rdfs:subClassOf fiaf:BroadcastStandard .

fiaf:PAL a owl:Class ;
    rdfs:label "PAL"@en ;
    rdfs:label "PAL"@es ;
    rdfs:label "PAL"@fr ;
    dc:source "FIAF Moving Image Cataloguing Manual 3.1.5.10"@en ;
    dc:source "Manual FIAF de Catalogación de Imágenes en Movimiento 3.1.5.10"@es ;
    dc:source "Manuel de catalogage des images animées de la FIAF 3.1.5.10"@fr ;
